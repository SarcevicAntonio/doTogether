{
	"name": "dotogether",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"dependency-report": "npx license-report --output=json > ./src/lib/dependency-report.json",
		"dev": "vite dev",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"postinstall": "svelte-kit sync && pnpm dependency-report",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"test": "playwright test"
	},
	"dependencies": {
		"@builder.io/mitosis": "^0.0.90",
		"@builder.io/mitosis-cli": "^0.0.49",
		"@firebase/app-types": "^0.x",
		"date-fns": "^2.29.3",
		"firebase": "^9.6.11",
		"firebase-admin": "^10.0.2",
		"js-confetti": "^0.11.0"
	},
	"devDependencies": {
		"@iconify/json": "^2.1.122",
		"@playwright/test": "^1.21.0",
		"@sveltejs/adapter-auto": "^1.0.0-next.83",
		"@sveltejs/kit": "^1.0.0-next.516",
		"@typescript-eslint/eslint-plugin": "^5.40.0",
		"@typescript-eslint/parser": "^5.40.0",
		"as-comps": "^0.0.24",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"svelte": "^3.52.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"tslib": "2.4.0",
		"typescript": "~4.6.2",
		"unplugin-icons": "^0.14.11",
		"vite": "^3.1.8"
	}
}
